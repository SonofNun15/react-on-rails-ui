{
  "name": "react-on-rails-ui",
  "version": "1.0.0",
  "description": "A front end application for interacting with react on rails.",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run security-check",
    "start": "npm-run-all --parallel lint:watch test:watch serve",
    "security-check": "nsp check",
    "lint": "esw build-scripts source --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec build-scripts/test-setup.js \"source/**/*.test.js\"",
    "test:debug": "npm run test -- --debug",
    "test:watch": "npm run test -- --watch",
    "serve": "webpack-dev-server --config webpack.config.dev.js --hot --progress --inline --host 0.0.0.0",
    "clean": "rimraf ./output && mkdir ./output",
    "prebuild": "npm-run-all clean lint test",
    "build": "webpack --config webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SonofNun15/react-on-rails-ui.git"
  },
  "keywords": [
    "react",
    "front-end",
    "sample"
  ],
  "author": "Josh Graber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SonofNun15/react-on-rails-ui/issues"
  },
  "homepage": "https://github.com/SonofNun15/react-on-rails-ui#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.10.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.7.1",
    "eslint-watch": "^2.1.14",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "node-sass": "^3.13.0",
    "npm-run-all": "^3.1.1",
    "nsp": "^2.6.2",
    "react-addons-test-utils": "^15.4.1",
    "redux-immutable-state-invariant": "^1.2.4",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "bootstrap": "^3.3.7",
    "es5-shim": "^4.5.9",
    "es6-promise": "^4.0.5",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "lodash": "^4.17.2",
    "moment": "^2.17.0",
    "numeral": "^1.5.6",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.0",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
}
